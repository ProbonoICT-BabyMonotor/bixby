result-view {
  match: message (this)
  
  message {
    template ("토큰 값을 확인해보세요.") {
      speech ("토큰 값을 확인해보세요.")
    }
  }

  render {
    list-of (this) {
      where-each (item) {
        macro (login-summary) {
          param (login) {
            expression (this)
          }
        }
      }
    }
  }
}